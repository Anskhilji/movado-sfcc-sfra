{
    "step-types": {
        "chunk-script-module-step": [
            {
                "@type-id":"custom.notifyBackInStockSubscribers",
                "@supports-organization-context": false,
                "@supports-parallel-execution":"false",
                "@supports-site-context":"true",
                "chunk-size": 20,
                "description": "Notify backinstock subscriber",
                "module": "int_custom_backinstock/cartridge/scripts/jobs/notifySubscribers.js",
                "before-step-function":"beforeStep",
                "total-count-function":"getTotalCount",
                "read-function":"read",
                "process-function":"process",
                "after-step-function":"afterStep",
                "parameters": {
                    "parameter": [{
                        "@name": "targetFolder",
                        "@type": "string",
                        "@required": "true",
                        "@trim": "true",
                        "description": "Target folder path (in IMPEX)",
                        "default-value": "/src/feeds/GoogleFeed/"
                    },
                    {
                        "@name": "fileName",
                        "description": "File name of the generated feed files",
                        "@type": "string",
                        "@required": "false",
                        "@trim": "true",
                        "default-value": "GoogleProductFeed"
                    }
                    ]
                },
                "status-codes": {
                    "status": [
                        {
                            "@code": "ERROR",
                            "description": "Used when an error occurred."
                        },
                        {
                            "@code": "OK",
                            "description": "Used when everything went well."
                        }
                    ]
                }
            }
        ]
    }
}