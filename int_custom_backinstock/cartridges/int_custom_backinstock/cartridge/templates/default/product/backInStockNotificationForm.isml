<isset name="product" value="${pdict.product}" scope="page" />
<div class="form-outer">
    <div class="back-in-stock-notification-container ${product.isBackInStockEnabled ? '' : 'd-none'}" data-pid="${product.id}"
        data-url="${URLUtils.url('BackInStockNotification-Subscribe')}">
        <isslot id="cs-pdp-back-in-stock-notification" context="global" description="Content slot for back in stock notification on pdp" />
    </div>
</div>
