
<isinclude sf-toolkit="off" template="/common/trackingPixelScript"/>
<script src="${URLUtils.staticURL('/js/libs.js')}"></script>
<script src="${URLUtils.staticURL('/lib/slick@1.8.1/slick.js')}"></script>
<script defer src="${URLUtils.staticURL('/lib/jquery-validation@1.19.0/jquery.validate.min.js')}"></script>
<script defer type="text/javascript" src="${URLUtils.staticURL('/js/main.js')}"></script>
<isif condition="${dw.system.Site.getCurrent().getCustomPreferenceValue('AffirmOnline')}">
	<script defer type="text/javascript" src="${URLUtils.staticURL('/js/vcn.js')}"></script>
</isif>
<isloop items="${ require('*/cartridge/scripts/assets.js').scripts }" var="script">
    <script defer type="text/javascript" src="${script}"></script>
</isloop>
<iscomment> This is used for s7.addthis.com cookies. Changed type and add class for consent cookies </iscomment>
<script type="text/plain" class="optanon-category-C0003-C0004" src="${dw.system.Site.getCurrent().preferences.custom.addthisUrl}"></script>



<isif condition="${dw.system.Site.current.preferences.custom.gtmEnabled}">
	<isinclude url="${URLUtils.url('GTM-LoadGTMTag')}" /> 
</isif>

<isinclude template="riskified/riskifedstorefrontbeacon.isml" sf-toolkit="off" />

<iscomment> Custom Start: Adding Marketing Cloud cartridge integration </iscomment>
<isset name="isMarketingCloudEnabled" value="${!empty(dw.system.Site.current.preferences.custom.marketingCloudModuleEnabled) ? dw.system.Site.current.preferences.custom.marketingCloudModuleEnabled : false}" scope="page" />
<isif condition="${isMarketingCloudEnabled}">
    <isscript>
        var hookHelper = require('*/cartridge/scripts/template/hookHelper');
        hookHelper.callHook('app.template.htmlHead', null, {pdict: pdict});
    </isscript>
</isif>
<iscomment> Custom End </iscomment>
<isif condition="${dw.system.Site.current.preferences.custom.enableUsablenetAccessibility}">
    <script type="text/javascript" src="${dw.system.Site.getCurrent().preferences.custom.usablenetAccessibilityStaticURL}"></script>
</isif>
