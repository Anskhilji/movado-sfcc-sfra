<isif condition="${dw.system.Site.current.preferences.custom.oneTrustCookieEnabled}">
    <isset name="oneTrustCookiesJS" value="${!empty(dw.system.Site.current.preferences.custom.oneTrustCookiesJS) ? dw.system.Site.current.preferences.custom.oneTrustCookiesJS : ''}" scope="page" />
    <isset name="oneTrustCookiesHtml" value="${!empty(dw.system.Site.current.preferences.custom.oneTrustCookieHtml) ? dw.system.Site.current.preferences.custom.oneTrustCookieHtml : ''}" scope="page" />
    <isprint value="${oneTrustCookiesJS}" encoding="off" />
    <div class="onetrust-sdk-cookie d-none">
        <isprint value="${oneTrustCookiesHtml}" encoding="off" />
    </div>
</isif>