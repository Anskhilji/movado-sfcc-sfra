<isloop items="${pdict.stores.stores}" var="store" status="loopState" end=98>
	<div  class="card-body store-marker" id="${store.ID}" >
		<div class="store-card-tile" tabindex="0" role="button" data-itemindex=${loopState.index}>
		<div class="map-marker">
			<span>${loopState.count}</span>
		</div>
		<isinclude template="storeLocator/storeDetails" />
		</div>
	</div>
</isloop>
