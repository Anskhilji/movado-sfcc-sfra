<div class="row hidden-xs-down breadcrumbs-wrapper-main">
    <div class="col col-space-inner">
        <ol class="breadcrumb">
            <isloop items="${pdict.breadcrumbs}" var="breadcrumbValue">
                <li class="breadcrumb-item">
                    <a href="${breadcrumbValue.url}">
                        ${breadcrumbValue.htmlValue}
                    </a>
                </li>
            </isloop>
        </ol>
    </div>
</div>
