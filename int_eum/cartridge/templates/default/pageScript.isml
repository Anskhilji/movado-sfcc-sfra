<isif condition="${pdict.userPageName !== null}">
	<script charset='UTF-8'>
	    (function(config) {
	        (function (info) {
	            info.PageView = function(context) {
	                return  {
	                    userPageName: "${pdict.userPageName}"
	                }
	            }
	         })(config.userEventInfo || (config.userEventInfo = {}));
	    })(window['adrum-config'] || (window['adrum-config'] = {}));
	</script>
</isif>