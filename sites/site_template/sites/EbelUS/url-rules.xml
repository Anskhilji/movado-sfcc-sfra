<?xml version="1.0" encoding="UTF-8"?>
<url-rules xmlns="http://www.demandware.com/xml/impex/urlrules/2012-12-01">
    <general-settings>
        <lower-case-urls>true</lower-case-urls>
        <blank-substitute>-</blank-substitute>
        <character-replacements-enabled>true</character-replacements-enabled>
        <character-replacements-for-page-url-enabled>true</character-replacements-for-page-url-enabled>
    </general-settings>
    <locale-settings>
        <type>path</type>
        <locale-mappings>
            <locale-mapping xml:lang="en-US">en</locale-mapping>
        </locale-mappings>
    </locale-settings>
    <category-rule>[ category-parent, [ attribute, ID ], / ]</category-rule>
    <category-page-url-override>false</category-page-url-override>
    <category-trailing-slash>true</category-trailing-slash>
    <category-offline-url-generation>true</category-offline-url-generation>
    <category-search-refinement-url-enabled>false</category-search-refinement-url-enabled>
    <product-rule>[[ constant, shop-watches ], /, [ attribute, name ]]</product-rule>
    <product-page-url-override>false</product-page-url-override>
    <product-id-separator>-</product-id-separator>
    <folder-rule>[ attribute, displayName]</folder-rule>
    <folder-page-url-override>true</folder-page-url-override>
    <folder-trailing-slash>true</folder-trailing-slash>
    <folder-offline-url-generation>true</folder-offline-url-generation>
    <folder-search-refinement-url-enabled>false</folder-search-refinement-url-enabled>
    <content-rule>[ attribute, displayName]</content-rule>
    <content-page-url-override>true</content-page-url-override>
    <content-id-separator>/</content-id-separator>
    <pipeline-aliases xml:lang="x-default">
        <pipeline-alias pipeline="Login-OAuthLogin">oauthlogin</pipeline-alias>
        <pipeline-alias pipeline="Wishlist-ShowOthers">wishlists</pipeline-alias>
        <pipeline-alias pipeline="Home-Show">home</pipeline-alias>
        <pipeline-alias pipeline="SiteMap-Start">sitemap</pipeline-alias>
        <pipeline-alias pipeline="Order-Confirm">confirmation</pipeline-alias>
        <pipeline-alias pipeline="Account-Show">account</pipeline-alias>
        <pipeline-alias pipeline="Address-AddAddress">addaddress</pipeline-alias>
        <pipeline-alias pipeline="Compare-Show">compare</pipeline-alias>
        <pipeline-alias pipeline="Login-Logout">logout</pipeline-alias>
        <pipeline-alias pipeline="Wishlist-AddProduct">wishlist-add</pipeline-alias>
        <pipeline-alias pipeline="Account-RegisterWatch">registerwatch</pipeline-alias>
        <pipeline-alias pipeline="Cart-Show">shopping-bag</pipeline-alias>
        <pipeline-alias pipeline="Account-EditProfile">account-edit</pipeline-alias>
        <pipeline-alias pipeline="Account-PasswordResetDialogForm">passwordresetdialog</pipeline-alias>
        <pipeline-alias pipeline="Account-SetNewPassword">setnewpassword</pipeline-alias>
        <pipeline-alias pipeline="Account-EditPassword">passwordchange</pipeline-alias>
        <pipeline-alias pipeline="Checkout-Begin">checkout</pipeline-alias>
        <pipeline-alias pipeline="Riskified-AnalysisNotificationEndpoint">riskified-notification</pipeline-alias>
        <pipeline-alias pipeline="Affirm-Success">affirmsuccess</pipeline-alias>
        <pipeline-alias pipeline="Address-List">address-list</pipeline-alias>
        <pipeline-alias pipeline="Account-ShowWatchlist">account-mywatches</pipeline-alias>
        <pipeline-alias pipeline="Order-Track">checkorder</pipeline-alias>
        <pipeline-alias pipeline="Home-ErrorNotFound">error</pipeline-alias>
        <pipeline-alias pipeline="Wishlist-Show">wishlist</pipeline-alias>
        <pipeline-alias pipeline="Order-History">order-history</pipeline-alias>
        <pipeline-alias pipeline="Login-Show">login</pipeline-alias>
        <pipeline-alias pipeline="Search-Show">search</pipeline-alias>
        <pipeline-alias pipeline="Stores-Find">stores-find</pipeline-alias>
    </pipeline-aliases>
    <pipeline-trailing-slash>false</pipeline-trailing-slash>
    <pipeline-perform-redirect>false</pipeline-perform-redirect>
    <character-replacements xml:lang="x-default">
        <character-replacement>
            <character>™</character>
            <replacement/>
        </character-replacement>
        <character-replacement>
            <character>"</character>
            <replacement/>
        </character-replacement>
        <character-replacement>
            <character>®</character>
            <replacement/>
        </character-replacement>
        <character-replacement>
            <character>'</character>
            <replacement/>
        </character-replacement>
        <character-replacement>
            <character>$</character>
            <replacement/>
        </character-replacement>
        <character-replacement>
            <character>&amp;</character>
            <replacement>and</replacement>
        </character-replacement>
    </character-replacements>
    <search-refinement-url-settings>
        <position>after</position>
        <segment-delimiter>slash</segment-delimiter>
        <refinements-delimiter>minus</refinements-delimiter>
        <value-delimiter>underscore</value-delimiter>
        <conflicting-value-delimiter>blank</conflicting-value-delimiter>
        <exclude-pipeline-alias-type>none</exclude-pipeline-alias-type>
        <product-name-identifier>prefn</product-name-identifier>
        <product-value-identifier>prefv</product-value-identifier>
        <content-name-identifier>crefn</content-name-identifier>
        <content-value-identifier>crefv</content-value-identifier>
    </search-refinement-url-settings>
</url-rules>

