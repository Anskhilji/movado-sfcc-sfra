{
    "step-types": {
        "script-module-step": [
            {
                "@type-id": "custom.SOM.parseOrderStatus",
                "@supports-site-context": true,
                "@supports-organization-context": true,
                "module": "int_movado_som/cartridge/scripts/jobs/parseOrderStatus.js",
                "function": "parseOrderStatus",
                "parameters": {
                    "parameters": [
                        {
                            "@name": "SourceFolder",
                            "@type": "string",
                            "@required": "true",
                            "@trim": "true",
                            "description": "Source folder path (in IMPEX)",
                            "default-value": "src/feed/orderstatus"
                        },
                        {
                            "@name": "FilePattern",
                            "description": "File pattern (Regular Expression)",
                            "@type": "string",
                            "@required": "false",
                            "@trim": "true",
                            "default-value": ".*\\.xml"
                        },
                        {
                            "@name": "IsDisabled",
                            "description": "Mark the step as disabled. This will skip the step and returns a OK status",
                            "@type": "boolean",
                            "@required": false,
                            "@trim": true
                        }
                    ]
                },
                "status-codes": {
                    "status": [
                        {
                            "@code": "ERROR",
                            "description": "Used when an error occurred."
                        },
                        {
                            "@code": "OK",
                            "description": "Used when everything went well."
                        },
                        {
                            "@code": "NO_FILES_FOUND",
                            "description": "Used when no files could be found in the source directory."
                        }
                    ]
                }
            },
            {
                "@type-id": "custom.SOM.exportFulfillmentOrder",
                "@supports-site-context": true,
